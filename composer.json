{
	"name": "keshwendu/ai-cms-grapesjs",
	"description": "Aimeos GrapesJS CMS extension",
	"keywords": ["aimeos", "extension", "cms", "grapesjs"],
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"prefer-stable": true,
	"minimum-stability": "dev",
	"repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/keshwendu/ai-admin-jqadm"
        }
    ],
	"require": {
		"php": "^8.0.11",
		"aimeos/aimeos-core": "dev-master",
		"keshwendu/ai-admin-jqadm": "dev-master",
		"aimeos/ai-admin-jsonadm": "dev-master",
		"aimeos/ai-client-html": "dev-master",
		"aimeos/ai-client-jsonapi": "dev-master",
		"aimeos/ai-controller-frontend": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "~10.0||~11.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	}
}